# This file demonstrates the #filter-unless directive

# Keep only specific keys (remove everything else)
#filter-unless EXISTING_KEY BASE_KEY

# Keep only keys matching wildcard patterns (remove everything else)
#filter-unless TEST_* *_KEY

# Keep only production-related keys
#filter-unless *_PROD PROD_*

# Keep only API keys with specific patterns
#filter-unless API_*_KEY *_API_*

# Add new variables
PRODUCTION_KEY=prod_value
FINAL_KEY=final_value
API_SECRET=secret_value
